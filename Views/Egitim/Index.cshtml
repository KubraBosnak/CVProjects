@using CvProject.Models.Entity
@model List<TblEgitimlerim>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<h2>Index</h2>
<br />
<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>OKUL / EĞİTİM KURUMU</th>
        <th>FAKÜLTE</th>
        <th>BÖLÜM</th>
        <th>GNO</th>
        <th>TARİH</th>
        <th>SİL</th>
        <th>DÜZENLE</th>

    </tr>
    @foreach (var item in Model)
    {
<tr>
    <td>@item.ID</td>
    <td>@item.Baslik</td>
    <td>@item.AltBaslik1</td>
    <td>@item.AltBaslik2</td>
    <td>@item.GNO</td>
    <td>@item.Tarih</td>
    <td><a class="btn btn-danger btn-sm" href="/Egitim/EgitimSil/@item.ID"><i class="fas fa-trash"></i> Sil</a></td>
    <td><a class="btn btn-info btn-sm" href="/Egitim/EgitimDuzenle/@item.ID"><i class="fas fa-pencil-alt"></i> Düzenle</a></td>

</tr>
    }
</table>
<a href="/Egitim/EgitimEkle" class="btn btn-primary">Yeni Eğitim Bilgisi Ekle</a>

